{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/daily10/challenge/:id",
      "destination": "/api/daily10/challenge/:id"
    },
    {
      "source": "/hands/:id",
      "destination": "/api/hands/:id"
    },
    {
      "source": "/invite/:inviter",
      "destination": "/api/invite/:inviter"
    }
  ],
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}
